{
  "name": "nestjs-api-structure",
  "version": "1.0.0",
  "description": "NestJS API structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint \"src/\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && eslint \"src/\" --fix",
    "format:all": "npx prisma format && prettier --write \"**/*.{ts,md,js,mjs,json,html}\" && eslint \"src/\" --fix",
    "build": "rimraf dist && npm run format:all && nest build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && npm run format:all && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "prisma:init": "npx prisma init",
    "prisma:dev": "npx prisma migrate dev --name ",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:status": "npx prisma migrate status",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:reset": "npx prisma migrate reset --force --skip-seed"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@as-integrations/fastify": "^2.1.1",
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/passport": "^3.0.2",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.0.4",
    "@keyv/redis": "^4.2.0",
    "@nestjs/apollo": "^13.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.1",
    "@nestjs/common": "^11.0.7",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.7",
    "@nestjs/graphql": "^13.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.7",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.0.7",
    "@nestjs/platform-socket.io": "^11.0.7",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.0.7",
    "@prisma/client": "6.3.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "bullmq": "^5.39.1",
    "cache-manager": "^6.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fastify-socket.io": "^5.1.0",
    "graphql": "^16.10.0",
    "ioredis": "^5.4.2",
    "kafkajs": "^2.2.4",
    "keyv": "^5.2.3",
    "nanoid": "^5.0.9",
    "node-forge": "^1.3.1",
    "passport": "^0.7.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "swc-loader": "^0.2.6",
    "@nestjs/cli": "^11.0.2",
    "@swc/core": "^1.10.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@types/node-forge": "^1.3.11",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "terser-webpack-plugin": "^5.3.11",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideongloomist/nestjs-api-structure"
