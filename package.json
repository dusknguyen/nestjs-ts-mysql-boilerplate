{
  "name": "nestjs-api-structure",
  "version": "1.0.0",
  "description": "NestJS api structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint \"src/\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && eslint \"src/\" --fix",
    "format:all": "prettier --write \"**/*.{ts,md,js,mjs,json,html}\" && eslint \"src/\" --fix",
    "build": "rimraf dist && npm run format:all && nest build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && npm run format:all && nest start --watch",
    "start:debug": "rimraf dist && npm run format:all && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "migration:create": "npx node ./scripts/migration/create.mjs",
    "migration:generate": "npx node ./scripts/migration/generate.mjs",
    "migration:run": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d=scripts/ormConfig.mjs",
    "migration:revert": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d=scripts/ormConfig.mjs"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/axios": "^3.1.1",
    "@nestjs/bull": "^10.2.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.7",
    "bull": "^4.16.4",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "graphql": "^16.9.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "mysql2": "^3.11.3",
    "nanoid": "^3.3.7",
    "nest-winston": "^1.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "randombytes": "^2.1.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.2.2",
    "@types/express-session": "^1.18.0",
    "@types/helmet": "^4.0.0",
    "@types/node": "^22.8.7",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/randombytes": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideongloomist/nestjs-api-structure"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/hideongloomist/nestjs-api-structure#readme",
  "author": "hideongloomist",
  "license": "MIT"
}
